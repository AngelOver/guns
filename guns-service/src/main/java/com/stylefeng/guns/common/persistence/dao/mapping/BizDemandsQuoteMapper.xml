		<id column="id" property="id" />
		<result column="price" property="price" />
		<result column="quote_time" property="quoteTime" />
		<result column="status" property="status" />
		<result column="demand_id" property="demandId" />
		<result column="agent_id" property="agentId" />
		<result column="passport_id" property="passportId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, price, quote_time AS quoteTime, status, demand_id AS demandId, agent_id AS agentId,passport_id AS passportId
    </sql>

</mapper>
