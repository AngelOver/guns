<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.BizContractMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.BizContract">
		<id column="id" property="id" />
		<result column="contract_type" property="contractType" />
		<result column="name" property="name" />
		<result column="address" property="address" />
		<result column="contacts" property="contacts" />
		<result column="contacts_phone" property="contactsPhone" />
		<result column="phone" property="phone" />
		<result column="email" property="email" />
		<result column="agent_id" property="agentId" />
		<result column="passport_id" property="passportId" />
		<result column="remark" property="remark" />
		<result column="create_time" property="createTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, contract_type AS contractType, name, address, contacts, contacts_phone AS contactsPhone, phone, email, agent_id AS agentId, passport_id AS passportId, remark, create_time AS createTime
    </sql>

</mapper>
