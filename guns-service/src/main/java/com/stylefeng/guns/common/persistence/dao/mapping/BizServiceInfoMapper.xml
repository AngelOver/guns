<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.BizServiceInfoMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.BizServiceInfo">
		<id column="id" property="id" />
		<result column="service_id" property="serviceId" />
		<result column="service_fileds_id" property="serviceFiledsId" />
		<result column="service_filed_value" property="serviceFiledValue" />
		<result column="service_filed_name" property="serviceFiledName" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, service_id AS serviceId, service_fileds_id AS serviceFiledsId, service_filed_value AS serviceFiledValue, service_filed_name AS serviceFiledName
    </sql>

</mapper>
